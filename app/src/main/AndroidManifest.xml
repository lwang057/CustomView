<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lwang.customview">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".waveview.WaveViewActivity" />
        <activity android:name=".stepqqview.StepQQViewActivity" />
        <activity android:name=".colortrackview.ColorTrackViewActivity" />
        <activity android:name=".lettersideview.LetterSideViewActivity" />
        <activity android:name=".slidingmenuview.SlidingMenuViewActivity" />
        <activity android:name=".indicatorseekbar.IndicatorSeekBarActivity" />
        <activity android:name=".lockpatternview.LockPatternViewActivity" />
    </application>

</manifest>